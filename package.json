{
    "name": "cojamru",
    "description": "Source code of cojam.ru website",
    "private": true,
    "version": "4.0.0",
    "author": "Dmitry <dmitry@cojam.ru> (https://e965.ru)",
    "repository": "github:cojamru/cojam.ru",
    "license": "MIT",
    "scripts": {
        "build": "rollup -c",
        "start": "rollup -c -w",
        "host": "sirv public",
        "validate": "svelte-check",
        "prettier": "prettier --write **/*.{html,json,js,ts,css,scss,svelte}",
        "lint-staged": "lint-staged"
    },
    "lint-staged": {
        "*.{html,json,js,ts,css,scss,svelte}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint-staged"
        }
    },
    "dependencies": {
        "svelte": "3.24.1",
        "svelte-icons": "2.1.0",
        "svelte-routing": "1.4.2"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "15.0.0",
        "@rollup/plugin-node-resolve": "9.0.0",
        "@rollup/plugin-typescript": "5.0.2",
        "@tsconfig/svelte": "1.0.0",
        "husky": "4.2.5",
        "lint-staged": "10.2.11",
        "prettier": "2.0.5",
        "rollup": "2.3.4",
        "rollup-plugin-livereload": "1.3.0",
        "rollup-plugin-svelte": "5.0.3",
        "rollup-plugin-terser": "7.0.0",
        "sass": "1.26.10",
        "sirv-cli": "1.0.6",
        "svelte-check": "1.0.0",
        "svelte-preprocess": "4.1.1",
        "tslib": "2.0.0",
        "typescript": "4.0.2"
    }
}
